{
  "name": "ember-data-model-fragments",
  "version": "6.0.10",
  "description": "Ember Data addon to support nested JSON documents",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-data",
    "ember-cli",
    "fragments"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adopted-ember-addons/ember-data-model-fragments.git"
  },
  "license": "MIT",
  "author": "Steven Lindberg <steven@lytics.io>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "npx release-it"
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^3.0.0",
    "calculate-cache-key-for-tree": "^1.1.0",
    "ember-cli-babel": "^8.2.0",
    "ember-compatibility-helpers": "^1.2.1",
    "ember-template-imports": "^4.3.0",
    "git-repo-info": "^2.1.1",
    "npm-git-info": "^1.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/macros": "^1.17.2",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.25.1",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.4.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.6.0",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.1",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-source": "^5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.15.0",
    "loader.js": "^4.7.0",
    "pretender": "^3.4.3",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^36.0.1",
    "webpack": "^5.99.7"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "resolutions": {
    "@embroider/addon-shim": "1.8.7",
    "@embroider/macros": "1.16.12"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "14.21.3",
    "yarn": "1.22.19"
  }
}
