{
  "name": "ember-data-model-fragments",
  "description": "Ember Data addon to support nested JSON documents",
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/lytics/ember-data.model-fragments.git"
  },
  "author": "Steven Lindberg <steven@lytics.io>",
  "license": "MIT",
  "scripts": {
    "build": "ember build",
    "build:production": "ember build --environment=production",
    "start": "ember serve",
    "test": "npm run build && ./node_modules/testem/testem.js ci"
  },
  "keywords": [
    "ember-addon",
    "ember",
    "ember-data",
    "ember-cli",
    "fragments"
  ],
  "main": "ember-addon/index.js",
